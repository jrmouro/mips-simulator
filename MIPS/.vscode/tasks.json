{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "-Wall",
                "-std=c++17",
                "*.cpp",
                "-o",
                "dist/Debug/GNU-Linux/mips"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Task generated by Debugger.",
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true
            }
        }
    ],
    "version": "2.0.0"
}